        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".selectimg.SelectImageActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".adapter.AdapterMainActivity" />
        <activity
            android:name=".adapter.HeaderFooterTest" />
